
<h2>Update the drone: "{{this.name}}"</h2>

<form action="/drones/{{this.drone._id}}/edit" method="post">
  <label> Name:
    <input type="text" name="drone" value="{{this.name}}" />
  </label>

  <label> Propellers:
    <input type="number" name="propellers" value="{{this.propellers}}" />
  </label>

  <label> Max Speed:
    <input type="number" name="maxSpeed" value="{{this.maxSpeed}}" />
  </label>

  

  <button type="submit">Save changes</button>
</form>